---
import Layout from "../layouts/Layout.astro";
---

<Layout title="Checking access…">
  <p>Checking access…</p>

  <script>
    import { requireInvited } from "../lib/requireInvite";

    (async () => {
      const ctx = await requireInvited();
      if (!ctx) return;

      window.location.replace("/rsvp");
    })();
  </script>
</Layout>
